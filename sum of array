#include<stdio.h>
int main()
{
    int arr[5];
    int sum = 0;

    for(int i = 0; i < 5; i++)
    {
        printf("Enter element %d: ", i + 1);
        scanf("%d", &arr[i]);
    }

    for(int j = 0; j < 5; j++)
    {
        printf("Element %d: %d\n", j + 1, arr[j]);
        sum += arr[j];
    }

    printf("Sum of the array elements: %d\n", sum);

    return 0;
}
